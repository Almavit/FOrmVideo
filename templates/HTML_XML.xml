<templateSet group="HTML/XML">
  <template name="{%" value="{% $SELECTION$ %}" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="{{" value="{{ $SELECTION$ }}" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="{" value="{% block $SELECTION$ %}{% endblock %}" shortcut="TAB" description="" toReformat="false" toShortenFQNames="true">
